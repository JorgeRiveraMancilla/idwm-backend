{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings":
  {
    "SqliteDatabase": "Data Source=app.db"
  },
  "CORS": {
    "PolicyName": "AllowAllOrigins",
    "AllowedOrigins": ["*"],
    "AllowedMethods": ["*"],
    "AllowedHeaders": ["*"],
    "AllowCredentials": true
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/myapp-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": ["FromLogContext"]
  },
  "Cloudinary": {
    "CloudName": "your_cloud_name",
    "ApiKey": "your_api_key",
    "ApiSecret": "your_api"
  },
  "JWTSecret": "your_jwt_secret",
  "Products": {
    "FewUnitsAvailable": 15,
    "ImageAllowedExtensions": [".jpg", ".jpeg", ".png", ".webp"],
    "ImageMaxSizeInBytes": 5242880
  },
  "User": {
    "AdminUser":
    {
      "Email": "your-admin-email",
      "Password": "your-admin-Password",
      "FirstName": "your-admin-FirstName",
      "LastName": "your-admin-LastName",
      "Gender": "your-admin-Gender",
      "Rut": "your-admin-Rut",
      "BirthDate": "your-admin-BirthDate",
      "PhoneNumber": "your-admin-PhoneNumber"
    },
    "RandomUserPassword": "your-User-Random-Password",
    "Genders": [
      "Masculino",
      "Femenino",
      "Otro"
    ]
  }
}
